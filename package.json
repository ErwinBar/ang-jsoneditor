{
  "name": "@erwin_by/ang-jsoneditor",
  "version": "6.0.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErwinBar/ang-jsoneditor.git"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run build:lib && ng build",
    "build:lib": "ng build --project ang-jsoneditor",
    "watch": "npm run build:lib -- --watch",
    "build:full": "npm run build:lib && npm run build:app",
    "publishnpm": "cp README.md projects/ang-jsoneditor/README.md && npm run build:lib && cd dist/ang-jsoneditor && npm publish",
    "unpublish": "npm deprecate -f 'ang-jsoneditor@1.9.3' 'this package has been deprecated'",
    "test": "npm run test:dev && npm run e2e",
    "test:dev": "ng test --project ang-jsoneditor-demo",
    "test:unit": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ports": "lsof -i tcp:4201",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i",
    "rebuildsass": "npm rebuild node-sass"
  },
  "dependencies": {
    "@angular/common": "^20.0.5",
    "@angular/compiler": "^20.0.5",
    "@angular/core": "^20.0.5",
    "@angular/forms": "^20.0.5",
    "@angular/platform-browser": "^20.0.5",
    "@angular/platform-browser-dynamic": "^20.0.5",
    "@angular/router": "^20.0.5",
    "jsoneditor": "9.10.2",
    "ng-packagr": "^20.0.1",
    "rxjs": "*",
    "tslib": "^2.3.0",
    "webpack": "^5.88.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.4",
    "@angular-devkit/schematics": "^20.0.4",
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/cli": "^20.0.4",
    "@angular/compiler-cli": "^20.0.5",
    "@angular/language-service": "^20.0.5",
    "@types/jasmine": "~5.1.2",
    "@types/jsoneditor": "^9.9.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cpr": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~5.1.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.8.3"
  },
  "description": "[![Build Status](https://travis-ci.org/ErwinBar/ang-jsoneditor.svg?branch=master)](https://travis-ci.org/ErwinBar/ang-jsoneditor)",
  "main": "index.js",
  "author": "",
  "bugs": {
    "url": "https://github.com/ErwinBar/ang-jsoneditor/issues"
  },
  "homepage": "https://github.com/ErwinBar/ang-jsoneditor#readme"
}
